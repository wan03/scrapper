<table class="table table-hover table-striped">
  <thead class="thead-dark">
    <tr>
      <th>#</th>
      <th>Product Name</th>
      <th>Price</th>
      <th>URL</th>
    </tr>
  </thead>
  <tbody>
    {{#each products}}
    <tr>
      <th scope="row">*</th>
      <td>{{title}}</td>
      <td>{{price}}</td>
      <td><a href="{{url}}"></a>Visit Alibaba</td>
      <td><a href="/product/{{_id}}">View Comments</a></td>
    </tr>
    {{/each}}

  </tbody>
</table>